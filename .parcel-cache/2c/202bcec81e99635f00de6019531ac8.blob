@import url("https://fonts.googleapis.com/css?family=Merriweather:400,700");
@import url(https://fonts.googleapis.com/css?family=Open+Sans:400,600,300);
.app-container {
  background-color: azure;
  padding-left: 120px;
  height: 100vh;
  top: 0;
  overflow: hidden;
}
.page-container {
  margin: 4em;
}
.page-container#home {
  text-align: center;
}
.page-container:not(#home) {
  display: none;
}
html {
  font-size: 100%;
}
/*16px*/
body {
  background: black;
  font-family: "Merriweather", serif;
  font-weight: 400;
  line-height: 1.75;
  color: #000000;
}
p {
  color: black;
  margin-bottom: 1rem;
}
h1, h2, h3, h4, h5 {
  margin: 3rem 0 1.38rem;
  color: black;
  font-family: "Merriweather", serif;
  font-weight: 700;
  line-height: 1.3;
}
h1 {
  margin-top: 0;
  font-size: 3.052rem;
}
h2 {
  font-size: 2.441rem;
}
h3 {
  font-size: 1.953rem;
}
h4 {
  font-size: 1.563rem;
}
h5 {
  font-size: 1.25rem;
}
small, .text_small {
  font-size: 0.8rem;
}
label {
  color: black;
}
nav {
  width: 120px;
  height: 100%;
  flex-direction: row;
  position: fixed;
  margin: 0;
  left: 0;
  top: -30px;
  padding-top: 50px;
  display: block;
  justify-content: space-around;
  align-items: center;
  padding-right: 50px;
  overflow: hidden;
}
nav .nav-links {
  list-style: none;
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  padding-right: 30px;
  background-color: #3e742d;
  flex-direction: column;
  align-items: center;
  overflow: hidden;
}
nav .nav-links .nav-item a {
  display: block;
  padding: 3em 0;
  margin-left: 20px;
  margin: auto;
  text-align: center;
  text-decoration: none;
  color: azure;
  overflow: hidden;
}
nav .nav-links .nav-item a:hover {
  color: #32322c;
}
nav .nav-links .nav-item a.active {
  font-weight: bold;
}
li {
  color: #3e742d;
  display: block;
  padding: 8px 16px;
  text-decoration: none;
}
li a:hover {
  background-color: #36f1cd;
  color: white;
}
#task-grid {
  display: grid;
  grid-template-columns: 1fr 1fr;
  grid-template-rows: 1fr;
  grid-template-areas: "taskform tasklist";
}
#taskform {
  grid-area: taskform;
  max-width: 300px;
  position: relative;
  left: 0px;
  margin: 0 auto;
}
#taskform label {
  display: block;
  width: 100%;
}
#tasklist {
  grid-area: tasklist;
}
#logo {
  width: 800px;
}
#favicon {
  width: 100px;
  align-self: center;
}
body {
  color: #000000;
}
.stopwatch {
  display: grid;
  justify-items: center;
  grid-row-gap: 23px;
  width: 100%;
  padding-top: 25px;
}
.circle {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 300px;
  width: 300px;
  border: 2px solid;
  border-radius: 50%;
}
.time {
  font-family: "Roboto Mono", monospace;
  font-weight: 300;
  font-size: 48px;
}
.studbud {
  font-weight: 900;
  color: #00ff22;
}
.controls {
  display: flex;
  justify-content: space-between;
  width: 187px;
}
.buttonPlay {
  cursor: pointer;
  background: transparent;
  padding: 0;
  border: none;
  margin: 0;
  outline: none;
  filter: invert(1);
}
.buttonReset {
  cursor: pointer;
  background: transparent;
  padding: 0;
  border: none;
  margin: 0;
  outline: none;
  filter: invert(70%);
}
#playButton {
  display: block;
}
#pauseButton {
  display: none;
}
#background-circle {
  height: 1000px;
  width: 1000px;
  background-color: #d4e5be;
  border-radius: 50%;
  position: relative;
  bottom: 850px;
  left: 10px;
  z-index: -1;
}
#musicbot {
  z-index: -1;
}
* {
  margin: 0;
  padding: 0;
  list-style: none;
  vertical-align: baseline;
}
body {
  background-color: #2A2C31;
  color: #FFFEDF;
  font-family: "Open Sans", sans-serif;
}
.center {
  text-shadow: 0px 1px 1px #191919;
  cursor: default;
  width: 450px;
  margin: auto;
  display: block;
  position: relative;
}
.center .title {
  padding-top: 10px;
  width: 100%;
  font-weight: 600;
  text-align: center;
  font-size: 45px;
  padding-bottom: 15px;
}
.center .settings {
  width: 350px;
  padding-bottom: 15px;
  display: table;
  margin: auto;
  position: relative;
  text-align: center;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
.center .settings .description {
  font-size: 15px;
}
.center .settings .value {
  font-size: 30px;
}
.center .settings .session {
  width: 108px;
  float: right;
}
.center .settings .session .description {
  color: #2ecc71;
}
.center .settings .break {
  width: 100px;
  float: left;
}
.center .settings .break .description {
  color: #e74c3c;
}
.center .settings .control {
  font-size: 33px;
  cursor: pointer;
}
.center .settings .control:hover {
  color: #95a5a6;
}
.center .next-pomodoro {
  margin-left: 30px;
  float: left;
  cursor: pointer;
  font-weight: bold;
}
.center .reset-pomodoro {
  margin-right: 30px;
  float: right;
  cursor: pointer;
  font-weight: bold;
}
.center .pomodoro {
  cursor: pointer;
  width: 300px;
  height: 300px;
  margin: auto;
  clear: both;
  border-radius: 50%;
  border: 3px #2ecc71 solid;
  position: relative;
  overflow: hidden;
  text-shadow: none;
}
.center .pomodoro .fill {
  width: 100%;
  height: 0%;
  position: absolute;
  left: 0;
}
.center .pomodoro .fill.session {
  bottom: 0;
  z-index: 2;
  background-color: #2ecc71;
}
.center .pomodoro .fill.break {
  top: 0;
  z-index: 3;
  background-color: #c0392b;
}
.center .pomodoro .time-left {
  top: 32%;
  left: 50%;
  margin-left: -125px;
  width: 250px;
  text-align: center;
  font-size: 60px;
  z-index: 4;
  position: absolute;
}
.center .pomodoro .status {
  bottom: 35%;
  left: 50%;
  margin-left: -125px;
  width: 250px;
  text-align: center;
  font-size: 15px;
  z-index: 4;
  position: absolute;
}
.footer {
  clear: both;
  width: 100%;
  padding-top: 40px;
  padding-bottom: 25px;
  font-size: 12px;
  text-align: center;
}
.footer a {
  color: #2ecc71;
  text-decoration: none;
  font-weight: 600;
}
